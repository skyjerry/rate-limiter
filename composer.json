{
  "name": "skyjerry/rate-limiter",
  "description": "A rate limiter library for PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "skyjerry",
      "email": "ldwangzhen@email.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-redis": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "RateLimiter\\": "src/RateLimiter"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RateLimiter\\Tests\\": "tests/RateLimiter",
      "RateLimiter\\Tests\\Storage\\": "tests/Storage"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}